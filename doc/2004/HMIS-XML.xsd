							<xsd:documentation xml:lang="en">HMIS Data and Technical Standards, July 30, 2004, Universal Data Element 2.9</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="ZIPQualityCode" type="hmis:zIPQualityCode" minOccurs="0">
