	<xsd:complexType name="personAddress">
		<xsd:complexContent>
			<xsd:extension base="hmis:personAddressBase">
				<xsd:attributeGroup ref="hmis:dateStampGroup"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>	
	
